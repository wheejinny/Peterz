function Obj_Class(class_target, class_name) {
  try {
    if (class_target == "") throw "TARGET IS EMPTY";
    if (class_name == "") throw "CLASS NAME IS EMPTY";
  } catch (err) {
    var this_func = get_func_name();
    console.log(this_func + ";ERROR;" + err);
  } finally {
    $("#" + class_target).addClass(class_name);
  }
}
